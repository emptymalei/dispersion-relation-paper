issn = {0031-9007},
journal = {Physical Review Letters},
mendeley-groups = {Neutrino/collective},
month = {jan},
number = {2},
pages = {021101},
title = {{Fast Pairwise Conversion of Supernova Neutrinos: A Dispersion Relation Approach}},
url = {http://arxiv.org/abs/1610.01612 http://link.aps.org/doi/10.1103/PhysRevLett.118.021101},
volume = {118},
year = {2017}
}

