@article{Izaguirre2016a,
abstract = {Collective pair conversion {\$}\backslashnu{\_}e\backslashbar\backslashnu{\_}e\backslashleftrightarrow \backslashnu{\_}{\{}x{\}}\backslashbar\backslashnu{\_}{\{}x{\}}{\$} by forward scattering, where {\$}x=\backslashmu{\$} or {\$}\backslashtau{\$}, may be generic for supernova neutrino transport. Depending on the local angular intensity of the electron lepton number carried by neutrinos, the conversion rate is "fast," i.e., of the order of {\$}\backslashsqrt{\{}2{\}}G{\_}{\{}\backslashrm{\{}F{\}}{\}}(n{\_}{\{}\backslashnu{\_}e{\}}{\{}-{\}}\backslash,n{\_}{\{}\backslashbar\backslashnu{\_}e{\}})\backslashgg\backslashDelta m{\^{}}2{\_}{\{}\backslashrm atm{\}}/2E{\$}. We present a novel approach to understand these phenomena: A dispersion relation for the frequency and wave number {\$}(\backslashOmega,\backslashbf{\{}K{\}}){\$} of disturbances in the mean field of {\$}\backslashnu{\_}e\backslashnu{\_}x{\$} flavor coherence. Run-away solutions occur in "dispersion gaps," i.e., in "forbidden" intervals of {\$}\backslashOmega{\$} and/or {\$}\backslashbf{\{}K{\}}{\$} where propagating plane waves do not exist. We stress that the actual solutions also depend on the initial and/or boundary conditions which need to be further investigated.},
